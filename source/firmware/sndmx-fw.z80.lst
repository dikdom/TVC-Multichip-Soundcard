C180 @A180   D3 70            OUT   (070h),a   
C182 @A182   3E 03            LD   a,003h   
C184 @A184   D3 71            OUT   (071h),a   
C186 @A186   3E C3            LD   a,0C3h   ; A register => Z80 assembly code of JP XXXX
C188 @A188   32 38 00         LD   (0038h),a   ; Interrupt address first byte
C18B @A18B   21 B7 C1         LD   hl,OWN_INTERRUPT   
C18E @A18E   22 39 00         LD   (0039h),hl   ; jump address in default interrupt routine
C191 @A191   FB               EI      ; enable interrupts. player is already initialized.
